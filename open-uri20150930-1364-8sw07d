	<script type="text/javascript">
		jQuery(document).ready(function($){
			var $post_tags = $('.post-tags'), $post_editor = $('.post-editor'), $readnext = $('.readnext-post');
			$(window).scroll(function() {
				var distanceTop = 0;
				if($post_tags.length > 0) {
					distanceTop = $post_tags.offset().top;
				} else if($post_editor.length > 0) {
					distanceTop = $post_editor.offset().top;
				}
				if(distanceTop > 0) {
					distanceTop = distanceTop - $(window).height();
					if ($(window).scrollTop() > distanceTop) {
						$readnext.animate({ opacity: 1 }, 500);
					}
